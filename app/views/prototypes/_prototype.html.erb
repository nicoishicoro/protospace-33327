<div class="card">
   <%= link_to image_tag(prototype.image, class: :card__img ), prototype_path(prototype.id), method: :get %>
    <div class="card__body">
      <%= link_to prototype.title, root_path, class: :card__title %>
      <p class="card__summary">
      <%= link_to prototype.catch_copy, root_path %>
      </p>
      <%= link_to prototype.user.name, user_path(prototype.user_id), method: :get, class: :card__user %>
    </div>
</div>